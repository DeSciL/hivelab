{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@5.3.8",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "js/bootstrap.min.js",
        "js/bootstrap.bundle.min.js",
        "css/bootstrap.min.css"
      ]
    },
    {
      "library": "bootstrap-icons@1.13.1",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "microsoft-signalr@9.0.6",
      "destination": "wwwroot/lib/microsoft-signalr/"
    },
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "cdnjs",
      "library": "highlight.js@11.11.1",
      "destination": "wwwroot/lib/highlight.js/",
      "files": [
        "highlight.min.js",
        "styles/atom-one-light.css",
        "styles/atom-one-light.min.css",
        "styles/atom-one-dark.min.css",
        "styles/atom-one-dark.css",
        "languages/powershell.min.js"
      ]
    }
  ]
}